{
  "name": "js-vulnerabilities-rules",
  "version": "1.0.0",
  "license": "commercial",
  "private": true,
  "config": {
    "PROTOC_GEN_TS_PATH": "",
    "PROTOC_OUT_DIR": "",
    "UCFG_PROTO_PATH": ""
  },
  "scripts": {
    "build": "yarn install && yarn check-format && yarn clean && yarn protoc && yarn compile",
    "clean": "tsc --build --clean",
    "check-format": "prettier --list-different \"{src,tests}/**/*.ts\"",
    "test": "jest --ci",
    "snapshot": "jest --coverage=false --ci --watch",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "compile": "tsc --build",
    "protoc": "node generate_proto.js"
  },
  "main": "dist/rules.js",
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/express": "4.16.0",
    "@types/google-protobuf": "3.7.4",
    "@types/jest": "23.3.2",
    "@types/mkdirp": "1.0.1",
    "@types/node": "10.17.28",
    "@types/semver": "7.3.1",
    "@types/tmp": "0.2.0",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/experimental-utils": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.19.0",
    "fs-extra": "7.0.0",
    "jest": "26.6.3",
    "jest-sonar-reporter": "1.3.0",
    "mock-fs": "4.14.0",
    "prettier": "2.0.5",
    "ts-jest": "26.5.1",
    "ts-protoc-gen": "0.13.0",
    "typescript": "4.3.4"
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "all",
    "singleQuote": true,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.json"
  ],
  "bundledDependencies": [
    "google-protobuf",
    "mkdirp",
    "tmp"
  ],
  "dependencies": {
    "mkdirp": "0.5.5",
    "tmp": "0.2.1"
  }
}
